<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: red;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.mariushubatschek.is.scheduling.ui.Controller">
   <top>
      <Button fx:id="runButton" mnemonicParsing="false" onAction="#run" text="Run" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <StackPane fx:id="layout" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: green;">
         <children>
            <Canvas fx:id="canvas" height="375.0" style="-fx-cursor: hand;" width="600.0" />
         </children>
      </StackPane>
   </center>
   <bottom>
      <Button fx:id="stopButton" mnemonicParsing="false" onAction="#stop" text="Stop" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="qTextField" maxWidth="1.7976931348623157E308" promptText="Q" />
            <TextField fx:id="alphaTextField" promptText="alpha" />
            <TextField fx:id="betaTextField" promptText="beta" />
            <TextField fx:id="rhoTextField" promptText="rho" />
            <Label fx:id="tourLengthLabel" maxWidth="1.7976931348623157E308" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>
   </right>
</BorderPane>
